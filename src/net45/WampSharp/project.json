{
  "title": "WampSharp",
  "version": "1.2.3-*",
  "description": "An implementation of WAMP (The Web Application Messaging Protocol). Supports both WAMPv2 and WAMPv1.",
  "authors": [ "CodeSharp" ],
  "packOptions": {
    "requireLicenseAcceptance": false,
    "tags": [ "websockets", "wampws", "rpc", "pubsub", "wampv2" ],
    "projectUrl": "https://github.com/Code-Sharp/WampSharp/"
  },
  "frameworks": {
    "net40": {
      "buildOptions": {
        "define": [ "NET40", "CASTLE", "LIBLOG_PUBLIC" ]
      },
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Core": "4.0.0.0"
      },
      "dependencies": {
        "Rx-Main": "2.2.5",
        "Castle.Core": "3.3.3",
        "Ix-Async": "1.2.5",
        "Rackspace.Collections.Immutable": "1.1.36"
      }
    },
    "net45": {
      "buildOptions": {
        "define": [ "NET45", "CASTLE", "LIBLOG_PUBLIC", "ASYNC" ]
      },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "4.0.0.0",
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Collections": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0"
      },
      "dependencies": {
        "System.Reactive": "3.0.0",
        "Castle.Core": "3.3.3",
        "Ix-Async": "1.2.5",
        "System.Collections.Immutable": "1.2.0"
      }
    },
    "net46": {
      "buildOptions": {
        "define": [ "NET45", "CASTLE", "LIBLOG_PUBLIC", "ASYNC", "ASYNC_LOCAL" ]
      },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "4.0.0.0",
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Collections": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0"
      },
      "dependencies": {
        "System.Reactive": "3.0.0",
        "Castle.Core": "3.3.3",
        "Ix-Async": "1.2.5",
        "System.Collections.Immutable": "1.2.0"
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile111": {
      "buildOptions": {
        "define": [ "NET45", "PCL", "LIBLOG_PUBLIC", "MANUAL_PROXY", "LIBLOG_PORTABLE", "TPL", "ASYNC" ]
      },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "",
        "System": "",
        "System.Core": "",
        "System.Collections": "",
        "System.Collections.Concurrent": "",
        "System.ComponentModel": "",
        "System.Diagnostics.Debug": "",
        "System.Dynamic.Runtime": "",
        "System.Globalization": "",
        "System.IO": "",
        "System.Linq": "",
        "System.Linq.Expressions": "",
        "System.Net.Primitives": "",
        "System.ObjectModel": "",
        "System.Reflection": "",
        "System.Reflection.Extensions": "",
        "System.Runtime": "",
        "System.Runtime.Extensions": "",
        "System.Runtime.InteropServices": "",
        "System.Runtime.Serialization": "",
        "System.Runtime.Serialization.Primitives": "",
        "System.Text.RegularExpressions": "",
        "System.Threading": "",
        "System.Threading.Tasks": ""
      },
      "dependencies": {
        "System.Reactive": "3.0.0",
        "Microsoft.Tpl.Dataflow": "4.5.24",
        "System.Collections.Immutable": "1.2.0"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NET45", "DNX", "PCL", "DISPATCH_PROXY", "MANUAL_PROXY", "LIBLOG_PUBLIC", "LIBLOG_PORTABLE", "TPL", "ASYNC_LOCAL", "ASYNC" ]
