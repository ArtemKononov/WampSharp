{
  "title": "WampSharp",
  "version": "1.2.3-*",
  "description": "An implementation of WAMP (The Web Application Messaging Protocol). Supports both WAMPv2 and WAMPv1.",
  "authors": [ "CodeSharp" ],
  "requireLicenseAcceptance": false,
  "tags": [ "websockets", "wampws", "rpc", "pubsub", "wampv2" ],
  "projectUrl": "https://github.com/Code-Sharp/WampSharp/",
  "frameworks": {
    "net40": {
      "compilationOptions": {
        "define": [ "NET40", "CASTLE", "LIBLOG_PUBLIC" ]
      },
      "frameworkAssemblies": {
        "System.Runtime.Serialization": "4.0.0.0",
        "System.Core": "4.0.0.0"
      },
      "dependencies": {
        "Rx-Main": "2.2.5",
        "Castle.Core": "3.3.1",
        "Ix-Async": "1.2.5",
        "Rackspace.Collections.Immutable": "1.1.36"
      }
    },
    "net45": {
      "compilationOptions": {
        "define": [ "NET45", "CASTLE", "LIBLOG_PUBLIC" ]
      },
      "frameworkAssemblies": {
        "Microsoft.CSharp": "4.0.0.0",
        "System": "4.0.0.0",
        "System.Core": "4.0.0.0",
        "System.Collections": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Runtime.Serialization": "4.0.0.0"
      },
      "dependencies": {
        "Rx-Main": "2.2.5",
        "Castle.Core": "3.3.1",
        "Ix-Async": "1.2.5",
        "System.Collections.Immutable": "1.1.36"
      }
    },
    "dotnet5.4": {
      "compilationOptions": {
        "define": [ "NET45", "DNX", "PCL", "LIBLOG_PUBLIC", "LIBLOG_PORTABLE", "TPL" ]
      },
      "dependencies": {
        "Microsoft.CSharp": "4.0.0",
        "Microsoft.Tpl.Dataflow": "4.5.24",
        "rx-main": "2.2.5",
        "System.Collections.Concurrent": "4.0.10",
        "System.Collections.Immutable": "1.1.37",
        "System.ComponentModel": "4.0.0",
        "System.Net.Primitives": "4.0.10",
        "System.Reflection.DispatchProxy": "4.0.0",
        "System.Runtime": "4.0.20",
        "System.Runtime.Serialization.Primitives": "4.0.10",
        "System.Text.RegularExpressions": "4.0.10"
      }
    }
  }
